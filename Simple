  local player = game.Players.LocalPlayer
  local PlayerPart
  
  -- Função para atualizar o PlayerPart quando o personagem é adicionado ou respawnado
  local function updatePlayerPart(character)
      PlayerPart = character:WaitForChild("HumanoidRootPart")
  end
  
  -- Se o personagem já estiver no jogo (caso o jogador já tenha spawnado), inicializa a referência
  if player.Character then
      updatePlayerPart(player.Character)
  end
  
  -- Conecta ao evento CharacterAdded para atualizar a referência quando o jogador respawnar
  player.CharacterAdded:Connect(updatePlayerPart)
  
  -- O resto do código permanece o mesmo
  
  
  
  local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
  local Frame = Instance.new("Frame", ScreenGui)
  local OpenMenu = Instance.new("ImageButton", ScreenGui)
  local TextBox = Instance.new("TextBox", Frame)
  local Texts = Instance.new("TextLabel", Frame)
  local TextButton = Instance.new("TextButton", Frame)
  
  local UICorner = Instance.new("UICorner", Frame)
  local UIStroke = Instance.new("UIStroke", Frame)
  local ButtonCorner = Instance.new("UICorner", TextButton) 
  local ButtonStroke = Instance.new("UIStroke", TextButton)
  local OpenCorner = Instance.new("UICorner", OpenMenu) 
  local OpenStroke = Instance.new("UIStroke", OpenMenu)
  local InputCorner = Instance.new("UICorner", TextBox) 
  local InputStroke = Instance.new("UIStroke", TextBox)
  
 
  OpenMenu.Size = UDim2.new(0, 40, 0, 40)
  OpenMenu.BackgroundColor3 = Color3.fromRGB(0,0,0)
  OpenMenu.Position = UDim2.new(0, 50, 0, 0)
  OpenMenu.Image = "rbxassetid://110106459736431"
  
  OpenCorner.CornerRadius = UDim.new(0, 10)-- Deixa o Frame com Bordas
  OpenStroke.Color = Color3.fromRGB(0, 0, 0)
  OpenStroke.Thickness = 0
  OpenStroke.ApplyStrokeMode = "Border"
  
  Frame.Size = UDim2.new(0, 200, 0, 200)
  Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
  Frame.Position = UDim2.new(0, 100, 0, 0)
  Frame.Transparency = 0
  
  UICorner.CornerRadius = UDim.new(0, 10)-- Deixa o Frame com Bordas
  UIStroke.Color = Color3.fromRGB(0, 0, 0)
  UIStroke.Thickness = 3
  UIStroke.ApplyStrokeMode = "Border"
  
  
  Texts.Text = "Teleport To Players"
  Texts.TextSize = 13
  Texts.Position = UDim2.new(0,100,0, 30)
  Texts.TextColor3 = Color3.fromRGB(255, 255, 255)
  
  
  TextBox.Size = UDim2.new(0, 200, 0, 30)
  TextBox.Position = UDim2.new(0, 0, 0, 70)
  TextBox.BackgroundColor3 = Color3.fromRGB(0,0,0)
  TextBox.TextColor3 = Color3.fromRGB(255,255,255)
  TextBox.TextSize = 12
