  TextBox.Text = ""
  TextBox.Transparency = 0.3
  
  InputCorner.CornerRadius = UDim.new(0, 0)-- Deixa o Frame com Bordas
  InputStroke.Color = Color3.fromRGB(0, 0, 0)
  InputStroke.Thickness = 0
  InputStroke.ApplyStrokeMode = "Border"
  
  
  TextButton.Text = "Click To Teleport"
  TextButton.BackgroundColor3 = Color3.fromRGB(0,0,0)
  TextButton.Size = UDim2.new(0, 150, 0, 50)
  TextButton.Position = UDim2.new(0, 25,0, 120)
  TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
  TextButton.Transparency = 0.3
  TextButton.TextSize = 10
  
  ButtonCorner.CornerRadius = UDim.new(0, 10)-- Deixa o Frame com Bordas
  ButtonStroke.Color = Color3.fromRGB(0, 0, 0)
  ButtonStroke.Thickness = 0
  ButtonStroke.ApplyStrokeMode = "Border"
  
  
  
  local function Minimize()
    
   Frame.Visible = not Frame.Visible
    
  end
  
  OpenMenu.Activated:Connect(Minimize)
  
  
  local function TP()
    local targetName = TextBox.Text
    local target = game.Players:FindFirstChild(targetName)
     if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
     
    local targetPos = target.Character.HumanoidRootPart.CFrame
    PlayerPart.CFrame = targetPos
    warn("ok")
     end
  end
  TextButton.Activated:Connect(TP)
